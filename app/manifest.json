{
    "manifest_version": 2,
    "name": "TxtNotes",
    "description": "Notes from txt files",
    "author": "Larry Wright",
    "packageId": "com.larrypdx.txtnotes",
    "version": "0.0.3",
    "minimum_chrome_version": "38",
    "icons": {
        "16": "images/icon_16.png",
        "128": "images/icon_128.png"
    },
    "app": {
        "background": {
            "scripts": [
                "scripts/background.js"
            ]
        }
    },
    "permissions": [
        "storage"
    ]
}